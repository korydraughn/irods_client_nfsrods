{
    "nfs_server": {
        "port": 2049,
        "irods_mount_point": "/tempZone",
        "user_information_refresh_time_in_milliseconds": 3600000,
        "file_information_refresh_time_in_milliseconds": 1000,
        "user_access_refresh_time_in_milliseconds": 1000
    },

    "irods_client": {
        "zone": "tempZone",
        "host": "<hostname>",
        "port": 1247,
        "default_resource": "demoResc",
        "ssl_negotiation_policy": "CS_NEG_REFUSE",
        "connection_timeout_in_seconds": 600,

        "connection_management": {
            "mode": "simple | cache | proxy",
            "cache_minimum_evictable_idle_time_in_milliseconds": 60000,
            "cache_soft_minimum_evictable_idle_time_in_milliseconds": 30000,
            "cache_maximum_idle_connections_per_key": 100,
            "cache_test_on_return": false,
            "cache_test_on_borrow": true,
            "cache_enable_jmx": false
        },

        "proxy_admin_account": {
            "username": "<username>",
            "password": "<password>"
        }
    }
}
